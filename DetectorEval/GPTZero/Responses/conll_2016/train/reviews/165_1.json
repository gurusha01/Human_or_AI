                },
                {
                    "generated_prob": 0.4357909560203552,
                    "sentence": "There are also relevant tools for biological phylogeny",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.553607165813446,
