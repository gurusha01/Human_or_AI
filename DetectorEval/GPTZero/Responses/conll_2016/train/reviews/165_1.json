                },
                {
                    "generated_prob": 0.4275268614292145,
                    "sentence": "Alexandre Bouchard-Cote, Tandy Warnow, Luay Nakhleh and Andrew Kitchen.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.29212862253189087,
                    "sentence": "Some",
