            "sentences": [
                {
                    "generated_prob": 0.3400551974773407,
                    "sentence": "This paper proposes a method for discovering correspondences between languages",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.32307878136634827,
                    "sentence": "based on MDL.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.3704678416252136,
                    "sentence": "The author model correspondences between words sharing the same",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.6056617498397827,
                    "sentence": "meaning in a number of Slavic languages.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.2859512269496918,
                    "sentence": "They develop codes for rules that",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.44432976841926575,
                    "sentence": "match substrings in two or more languages and formulate an MDL objective that",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.3573364019393921,
                    "sentence": "balances the description of the model and the data given the model.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.43383392691612244,
                    "sentence": "The model is trained with EM and tested on a set of 13 Slavic languages.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.3469112813472748,
                    "sentence": "The",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.3081193268299103,
                    "sentence": "results are shown by several distance measures, a phylogenetic tree, and",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.49201083183288574,
                    "sentence": "example of found correspondences.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.21991443634033203,
                    "sentence": "The motivation and formulation of the approach makes sense.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.25989407300949097,
                    "sentence": "MDL seems like a",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.2854694426059723,
                    "sentence": "reasonable tool to attack the problem and the motivation for employing EM is",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.39317846298217773,
                    "sentence": "presented nicely.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.18079142272472382,
                    "sentence": "I must admit, though, that some of the derivations were not",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.3884696066379547,
                    "sentence": "entirely clear to me.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.3958357870578766,
                    "sentence": "The authors point out the resemblance of the MDL objective to Bayesian",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.3863445222377777,
                    "sentence": "inference, and one thinks of the application of Bayesian inference in",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.2540343403816223,
                    "sentence": "(biological) phylogenetic inference, e.g.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.3968632221221924,
                    "sentence": "using the MrBayes tool.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.3509747087955475,
                    "sentence": "An empirical",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.25612732768058777,
                    "sentence": "comparison here could be insightful.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.3869849443435669,
                    "sentence": "Related work:",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.5007431507110596,
                    "sentence": "- Lacking comparison to methods for borrowing and cognate detection or other",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.41177111864089966,
                    "sentence": "computational methods for historical linguistics.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
