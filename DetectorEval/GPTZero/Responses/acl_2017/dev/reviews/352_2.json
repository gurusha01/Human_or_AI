                },
                {
                    "generated_prob": 0.003189230803400278,
                    "sentence": "The models",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.003697700332850218,
                    "sentence": "evaluates are a fully-shared, shared-private and adversarial shared-private --",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0039499965496361256,
                    "sentence": "the lattermost ASP model is one of the main contributions.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0008409033762291074,
                    "sentence": "They also use",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0056260330602526665,
