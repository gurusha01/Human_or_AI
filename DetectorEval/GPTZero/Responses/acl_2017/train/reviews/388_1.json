            "sentences": [
                {
                    "generated_prob": 0.02402816340327263,
                    "sentence": "This paper describes interesting and ambitious work: the automated conversion",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.018915168941020966,
                    "sentence": "of Universal Dependency grammar structures into [what the paper calls] semantic",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.019209733232855797,
                    "sentence": "logical form representations.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.01939885877072811,
                    "sentence": "In essence, each UD construct is assigned a",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.014359003864228725,
                    "sentence": "target construction in logical form, and a procedure is defined to effect the",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.021712249144911766,
                    "sentence": "conversion, working 'inside-out' using an intermediate form to ensure",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.023274477571249008,
                    "sentence": "proper nesting of substructures into encapsulating ones.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.015234394930303097,
                    "sentence": "Two evaluations are",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.024913568049669266,
                    "sentence": "carried out: comparing the results to gold-standard lambda structures and",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.03485581651329994,
                    "sentence": "measuring the effectiveness of the resulting lambda expressions in actually",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0263480506837368,
                    "sentence": "delivering the answers to questions from two QA sets.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.030391599982976913,
                    "sentence": "It is impossible to describe all this adequately in the space provided.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.03475898504257202,
                    "sentence": "The",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.031872160732746124,
                    "sentence": "authors have taken some care to cover all principal parts, but there are still",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0429898202419281,
                    "sentence": "many missing details.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0662827268242836,
                    "sentence": "I would love to see a longer version of the paper!",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0814739316701889,
                    "sentence": "Particularly the QA results are short-changed; it would have been nice to learn",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.07164935022592545,
                    "sentence": "which types of question are not handled, and which are not answered correctly,",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.04088877886533737,
                    "sentence": "and why not.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.09715037047863007,
                    "sentence": "This information would have been useful to gaining better insight",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.060920700430870056,
                    "sentence": "into the limitations of the logical form representations.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.07660508155822754,
                    "sentence": "That leads to my main concern/objection.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.07020982354879379,
                    "sentence": "This logical form representation is",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.06516658514738083,
                    "sentence": "not in fact a 'real' semantic one.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.1883176565170288,
                    "sentence": "It is, essentially, a",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.14361730217933655,
                    "sentence": "rather",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.20747072994709015,
                    "sentence": "close",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.10876046121120453,
                    "sentence": "rewrite of the dependency structure of the input, with some (good) steps toward",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.04958626627922058,
                    "sentence": "'semanticization', including the insertion of lambda operators, the",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.06317083537578583,
                    "sentence": "explicit inclusion of dropped arguments (via the enhancement operation), and",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.052857231348752975,
                    "sentence": "the introduction of appropriate types/units for such constructions as eventive",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.051794447004795074,
                    "sentence": "adjectives and nouns like \"running horse\" and \"president in 2009\".",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.03897079825401306,
                    "sentence": "But",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.033695489168167114,
                    "sentence": "many (even simple) aspects of semantic are either not present (at least, not in",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.024925168603658676,
                    "sentence": "the paper) and/or simply wrong.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.015653377398848534,
                    "sentence": "Missing: quantification (as in \"every\" or",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.017626449465751648,
                    "sentence": "\"all\"); numbers (as in \"20\" or \"just over 1000\"); various forms of",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.020452920347452164,
                    "sentence": "reference (as in \"he\", \"that man\", \"what I said before\"); negation",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0465705506503582,
                    "sentence": "and modals, which change the semantics in interesting ways; inter-event",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.03680916130542755,
                    "sentence": "relationships (as in the subevent relationship between the events in \"the",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.04205659031867981,
                    "sentence": "vacation was nice, but traveling was a pain\"; etc.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.05778307467699051,
                    "sentence": "etc.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.03562221676111221,
                    "sentence": "To add them one can",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.03987106680870056,
                    "sentence": "easily cheat, by treating these items as if they were just unusual words and",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.08226262778043747,
                    "sentence": "defining obvious and simple lambda formulas for them.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.036368973553180695,
                    "sentence": "But they in fact require",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.04364345967769623,
                    "sentence": "specific treatment; for example, a number requires the creation of a separate",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.05032049119472504,
                    "sentence": "set object in the representation, with its own canonical variable (allowing",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.03360255807638168,
                    "sentence": "later text to refer to \"one of them\" and bind the variable properly).",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.057061322033405304,
                    "sentence": "For",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.03436443582177162,
                    "sentence": "another example, Person A's model of an event may differ from Person B's,",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.05793203413486481,
                    "sentence": "so one needs two representation symbols for the event, plus a coupling and",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.036957576870918274,
                    "sentence": "mapping between them.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.03675120323896408,
                    "sentence": "For another example, one has to be able to handle time,",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.048079654574394226,
                    "sentence": "even if simply by temporally indexing events and states.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.017389405518770218,
                    "sentence": "None of this is here,",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.028620384633541107,
                    "sentence": "and it is not immediately obvious how this would be added.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.02522343024611473,
                    "sentence": "In some cases, as",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0750318169593811,
                    "sentence": "DRT shows, quantifier and referential scoping is not trivial.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.020707663148641586,
                    "sentence": "It is easy to point to missing things, and unfair to the paper in some sense;",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.016328420490026474,
                    "sentence": "you can't be expected to do it all.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.027747487649321556,
                    "sentence": "But you cannot be allowed to make",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.12162864208221436,
                    "sentence": "obvious errors.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.024567753076553345,
                    "sentence": "Very disturbing is the assignment of event relations strictly",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.03724003955721855,
                    "sentence": "in parallel with the verb's (or noun's) syntactic roles.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.014673739671707153,
                    "sentence": "No-one can claim",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.009136567823588848,
                    "sentence": "seriously that \"he broke the window\" and \"the window broke\" has",
