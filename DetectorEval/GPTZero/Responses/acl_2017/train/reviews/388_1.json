                },
                {
                    "generated_prob": 0.011848347261548042,
                    "sentence": "adequate treatment, even in this paper.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.00782776065170765,
                    "sentence": "This is to my mind the principal",
                    "perplexity": 0,
