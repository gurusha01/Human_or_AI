                {
                    "generated_prob": 0.06762797385454178,
                    "sentence": "The evaluation of the proposed system is not satisfying in several aspects.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.11932390183210373,
                    "sentence": "First, the annotators should have independently annotated a gold standard for",
                    "perplexity": 0,
