                },
                {
                    "generated_prob": 0.009064693003892899,
                    "sentence": "- General Discussion:",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.010585583746433258,
                    "sentence": "The POS tagging is left as more of an afterthought.",
                    "perplexity": 0,
