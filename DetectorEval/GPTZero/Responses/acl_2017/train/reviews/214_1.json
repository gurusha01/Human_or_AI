                },
                {
                    "generated_prob": 0.0005557629629038274,
                    "sentence": "combination of macro discourse structure and micro discourse structure.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.001539134420454502,
                    "sentence": "There,",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006105821230448782,
                    "sentence": "the micro discourse structure is presented within paragraphs.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0005410081357695162,
                    "sentence": "However, in the",
                    "perplexity": 0,
