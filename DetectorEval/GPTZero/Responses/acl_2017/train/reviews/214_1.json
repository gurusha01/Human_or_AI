                {
                    "generated_prob": 0.0006505043711513281,
                    "sentence": "proofread the paper.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0006951334653422236,
                    "sentence": "Regarding the content, I have several concerns:",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0015475514810532331,
                    "sentence": "1 The logic of the paper is not clear and justifiable:",
