                },
                {
                    "generated_prob": 0.0013227672316133976,
                    "sentence": "The paper is poorly written and I have difficulties to follow it.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.0020235113333910704,
                    "sentence": "I strongly",
