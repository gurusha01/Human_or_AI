                {
                    "generated_prob": 0.014311322942376137,
                    "sentence": "breaks things catastrophically.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.01279002707451582,
                    "sentence": "Another concern is the use of NeuralTalk to select the \"hardest\"",
