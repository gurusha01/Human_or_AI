                {
                    "generated_prob": 0.0884435772895813,
                    "sentence": "additional bootstrapping is not.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.27664875984191895,
                    "sentence": "While it is certainly interesting to experiment with neural networks on this",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.29490670561790466,
                    "sentence": "task, the merits of the proposed system are not entirely convincing.",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.112456776201725,
                    "sentence": "It seems",
                    "perplexity": 0,
                    "highlight_sentence_for_ai": false
                },
                {
                    "generated_prob": 0.2730918824672699,
